{
  "publisher": "TabNine",
  "icon": "small_logo.png",
  "author": "Tabnine <support@tabnine.com> (https://tabnine.com/)",
  "license": "License at https://tabnine.com/eula",
  "galleryBanner": {
    "color": "#100f11",
    "theme": "dark"
  },
  "name": "tabnine-vscode-self-hosted-updater",
  "displayName": "Tabnine Self Hosted Updater",
  "description": "Update Tabnine plugin from your self hosted server",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "vsce:package": "vsce package",
    "prettier": "prettier --write src/",
    "prettier:check": "prettier --check src/"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/semver": "^7.3.13",
    "@types/tmp": "^0.2.3",
    "@types/vscode": "^1.75.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "vsce": "^2.15.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "contributes": {
    "configuration": [
      {
        "title": "Tabnine Self Hosted Updater",
        "properties": {
          "tabnineSelfHostedUpdater.tabnineServer": {
            "type": "string",
            "description": "Tabnine's Self Hosted Server URL"
          }
        }
      }
    ]
  },
  "dependencies": {
    "axios": "^1.3.4",
    "debug": "^4.3.4",
    "semver": "^7.3.8",
    "tmp": "^0.2.1"
  }
}
